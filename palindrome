import java.util.*;
class Main{
    public static void main(String[] args){
        String s="Anna";
        s=s.toLowerCase();
        char [] c=s.toCharArray();
        String check="";
        for(int i=c.length-1;i>=0;i--){
            check+=c[i];
        }
        if(s.equals(check)){
            System.out.println("Palindrome");
        }else{
            System.out.println("not Palindrome");
        }
    }
}

import java.util.Scanner;

class Palindrome{

   public static void main(String[] args){
      Scanner scanner=new Scanner(System.in);
      int num=scanner.nextInt();
       int sum=0,r;
        int temp=num;
        while(num!=0){
            r=num%10;
            sum=sum*10+r;
            num/=10;
        }
      if(temp==sum){
          System.out.println("palindrome");
      }else{
          System.out.println("no palindrome");
      }
      
       
   }
}

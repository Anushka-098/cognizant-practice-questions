import java.util.Scanner;

class Character{
    public static void main(String[] args){
        int [] arr;
        
        Scanner scanner=new Scanner(System.in);
        System.out.println("Enter the digits:");
        arr=new int[4];
        for(int i=0;i<4;i++){
            arr[i]=scanner.nextInt();
             System.out.print(arr[i]+"-"+(char)arr[i]);
        }
       
    }
}
